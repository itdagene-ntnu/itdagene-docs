{
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react"
  ],
  "globals": {
    "log": true,
    "expect": true,
    "page": true,
    "browser": true,
  },
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "amd": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
}
}
